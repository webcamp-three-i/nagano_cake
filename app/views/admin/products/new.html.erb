<div class="container", style="padding-left: 350px;">

  <h2>商品新規登録</h2>

  <%= form_for @product, url: admin_products_path do |f| %>
    <%= render 'form', product: @product, f: f %>
    <div class="product-form", style="float: right;">
      <%= f.submit "新規登録", class: "btn btn-primary" %>
    </div>
  <% end %>

</div>